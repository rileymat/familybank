{{>header}}
 <div id="wrapper">
        {{>navigation}}
        <div id="page-wrapper">
		  <div class="row">
                <div class="col-lg-12">
                   <h1 class="page-header"> <i class="fa fa-bank fa-fw"></i> Account Details</h1>
                </div>
                <!-- /.col-lg-12 -->
          </div>
		  {{>subpage_nav}}
            <!-- /.row -->
			<div class="row">
			  <div class="col-lg-8">
				   <div class="panel panel-default">
                        <div class="panel-heading">
                            Transactions
                        </div>
                        <div class="panel-body">
						  <div class="row">
							<div class="details-col-header col-xs-3">
								Time
							  </div>
							  <div class="details-col-header col-xs-6">
								Description
							  </div>
							  <div class="details-col-header col-xs-3">
								Amount
							  </div>
							</div>

						  {{#transactions}}
							<div class="row transaction-detail">
							  <div class="col-xs-3">
								<div class="details-datetime">
								  <div class="details-date">{{date}}</div> <div class="details-time">{{time}}</div>
								</div>
							  </div>
							  <div class="col-xs-6 details-description">
								{{description}}
							  </div>
							  <div class="col-xs-3 details-amount">
								{{amount}}
							  </div>
							</div>
						  {{/transactions}}
						</div>
                    </div>
			  </div>
            </div>
        </div>
    </div>
{{>footer}}
